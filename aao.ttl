# baseURI: https://linked.data.gov.au/ont/aao
# imports: http://purl.org/dc/elements/1.1/
# imports: http://purl.org/pav/
# imports: http://www.w3.org/2006/time
# imports: http://www.w3.org/ns/org
# imports: http://www.w3.org/ns/prov-o#
# imports: http://xmlns.com/foaf/0.1/

@prefix aao: <https://linked.data.gov.au/ont/aao/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix organization: <http://ontology.eil.utoronto.ca/organization.owl> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

<https://linked.data.gov.au/ont/aao>
  rdf:type owl:Ontology ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
  owl:imports <http://purl.org/pav/> ;
  owl:imports <http://www.w3.org/2006/time> ;
  owl:imports <http://www.w3.org/ns/org> ;
  owl:imports <http://www.w3.org/ns/prov-o#> ;
  owl:imports foaf: ;
  dcterms:publisher <http://catalogue.linked.data.gov.au/org/O-000886> ;
  dc:publisher "CSIRO" .
.
aao:AAO
  rdf:type owl:Class ;
  rdfs:label "Administrative Arrangements Order" ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:date ;
      owl:onProperty dcterms:modified ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:date ;
      owl:onProperty aao:dateOfRegistration ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom aao:AAO ;
      owl:onProperty dcterms:isReplacedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom aao:AAO_Part ;
      owl:onProperty dcterms:hasPart ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:currentlyInForce ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:dateOfRegistration ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:intervalInForce ;
      skos:scopeNote """Interval in force: 
- begins on the date this AAO is signed by the G-G - aka date-issued
- ends one the day before the beginning of the AAO which replaces this one - aka date repealed""" ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:regID ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty dcterms:isReplacedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty dcterms:hasPart ;
    ] ;
.
aao:AAO_Amendment
  rdf:type owl:Class ;
  rdfs:label "Administrative Arrangements Order Amendment" ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:date ;
      owl:onProperty dcterms:issued ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:date ;
      owl:onProperty aao:dateOfRegistration ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom aao:AAO_Part ;
      owl:onProperty dcterms:hasPart ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty dcterms:issued ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:dateOfRegistration ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty dcterms:hasPart ;
    ] ;
.
aao:AAO_Part
  rdf:type owl:Class ;
  rdfs:label "AAO Part - usually concerned with one Department" ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              aao:AAO
              aao:AAO_Amendment
            ) ;
        ] ;
      owl:onProperty dcterms:isPartOf ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty dcterms:isPartOf ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:administeredLegislation ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:matterDealtWith ;
    ] ;
.
aao:Act
  rdf:type owl:Class ;
  rdfs:label "Act of Parliament" ;
  rdfs:subClassOf owl:Thing ;
.
aao:Act_Part
  rdf:type owl:Class ;
  rdfs:label "Part of an Act of Parlianment" ;
.
aao:Department
  rdf:type owl:Class ;
  rdfs:label "Department of State" ;
  rdfs:subClassOf org:Organization ;
  rdfs:subClassOf prov:Agent ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:partOfPortfolio ;
    ] ;
.
aao:Legislation
  rdf:type owl:Class ;
  rdfs:label "Legislation" ;
  rdfs:subClassOf prov:Entity ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty aao:includedLegislation ;
    ] ;
  skos:scopeNote "Association class used to specify an item of Legislation administered by a Minister, optionally with exceptions as noted" ;
.
aao:Matter
  rdf:type owl:Class ;
  rdfs:label "A matter dealt with by a department" ;
  rdfs:subClassOf prov:Activity ;
.
aao:Portfolio
  rdf:type owl:Class ;
  rdfs:label "Portfolio of government" ;
.
aao:Regulation
  rdf:type owl:Class ;
  rdfs:label "Regulation" ;
.
aao:administeredLegislation
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          aao:AAO_Part
          aao:Department
        ) ;
    ] ;
  rdfs:range aao:Legislation ;
.
aao:amendedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain aao:AAO ;
  rdfs:range aao:AAO_Amendment ;
.
aao:amends
  rdf:type owl:ObjectProperty ;
  rdfs:domain aao:AAO_Amendment ;
  rdfs:range aao:AAO ;
  owl:inverseOf aao:amendedBy ;
.
aao:currentlyInForce
  rdf:type owl:DatatypeProperty ;
  rdfs:domain aao:AAO ;
  rdfs:label "Status of this AAO" ;
  rdfs:range xsd:boolean ;
.
aao:dateOfRegistration
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Date when the AAO was added to the registry at legislation.gov.au" ;
  rdfs:subPropertyOf dcterms:date ;
.
aao:exceptionToLegislation
  rdf:type owl:ObjectProperty ;
  rdfs:domain aao:Legislation ;
  rdfs:range aao:Act_Part ;
.
aao:includedLegislation
  rdf:type owl:ObjectProperty ;
  rdfs:domain aao:Legislation ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          aao:Act
          aao:Act_Part
        ) ;
    ] ;
.
aao:intervalInForce
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          aao:AAO
          aao:AAO_Amendment
        ) ;
    ] ;
  rdfs:label "Time interval when the AAO was in force" ;
  rdfs:range time:ProperInterval ;
.
aao:matterDealtWith
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          aao:AAO_Part
          aao:Department
        ) ;
    ] ;
  rdfs:range aao:Matter ;
.
aao:partOfPortfolio
  rdf:type owl:ObjectProperty ;
  rdfs:domain aao:Department ;
  rdfs:range aao:Portfolio ;
.
aao:regID
  rdf:type owl:AnnotationProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          aao:AAO
          aao:AAO_Amendment
          aao:Legislation
        ) ;
    ] ;
  rdfs:label "Official identifier in the Australian Government Federal Register of Legislation" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf dcterms:identifier ;
  rdfs:subPropertyOf skos:notation ;
.
aao:relatedDepartment
  rdf:type owl:InverseFunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain aao:AAO_Part ;
  rdfs:range aao:Department ;
.
